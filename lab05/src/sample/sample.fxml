<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10">
    <TableView fx:id="tableView" GridPane.rowIndex="0" GridPane.columnIndex="0">
        <columns>
            <TableColumn fx:id="studentID" text="Student ID"></TableColumn>
            <TableColumn fx:id="midterm" text="midterm"></TableColumn>
            <TableColumn fx:id="finalExam" text="finalExam"></TableColumn>
            <TableColumn fx:id="assignments" text="assignments"></TableColumn>
            <TableColumn fx:id="finalMark" text="finalMark"></TableColumn>
            <TableColumn fx:id="letterGrade" text="letterGrade"></TableColumn>

        </columns>
    </TableView>
</GridPane>