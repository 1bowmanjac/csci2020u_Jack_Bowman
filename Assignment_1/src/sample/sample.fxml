<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="CENTER" hgap="10" vgap="10">
    <TableView fx:id="tableView" GridPane.rowIndex="0" GridPane.columnIndex="0" minWidth="800" minHeight="500">
        <columns>
            <TableColumn fx:id="filename" text="filename"></TableColumn>
            <TableColumn fx:id="actualClass" text="ActualClass"></TableColumn>
            <TableColumn fx:id="spamProbability" text="Spam Probability"></TableColumn>
        </columns>
    </TableView>

    <Label fx:id="accuracyLabel" text="username" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    <Label fx:id="precisionLabel" text="password" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
</GridPane>